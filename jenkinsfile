pipeline{

    agent any

    stages {
        stage('build') {
            steps {
                echo 'building new application'
            }
        }
        stage('test')
        {
            steps{
                echo 'running tests'
                 }
            
        }
                stage('deployer'){
                    steps{
                        echo'deploying the application'
                    }
                }
    }
        
    }
